	"item": [
		{
			"name": "Create Special Contract",
			"item": [
				{
					"name": "Create All Access Policy",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "X-Api-Key",
								"value": "password",
								"type": "text"
							},
							{
								"key": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"@context\": {\n    \"odrl\": \"http://www.w3.org/ns/odrl/2/\"\n  },\n  \"@type\": \"PolicyDefinitionRequest\",\n  \"@id\": \"{{ACCESS_POLICY_ID}}\",\n  \"policy\": {\n    \"@type\": \"Policy\",\n    \"odrl:permission\": [\n      {\n        \"odrl:action\": \"use\"\n      }\n    ]\n  }\n}"
						},
						"url": {
							"raw": "{{ALICE_MANAGEMENT}}/v2/policydefinitions",
							"host": [
								"{{ALICE_MANAGEMENT}}"
							],
							"path": [
								"v2",
								"policydefinitions"
							]
						}
					},
					"response": []
				},
				{
					"name": "Create Contract",
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"@context\": {},\r\n  \"@id\": \"{{CONTRACT_ID}}\",\r\n  \"@type\": \"ContractDefinition\",\r\n  \"accessPolicyId\": \"{{ACCESS_POLICY_ID}}\",\r\n  \"contractPolicyId\": \"{{CONTRACT_POLICY_ID}}\",\r\n  \"assetsSelector\": {\r\n    \"@type\": \"CriterionDto\",\r\n    \"operandLeft\": \"https://w3id.org/edc/v0.0.1/ns/id\",\r\n    \"operator\": \"=\",\r\n    \"operandRight\": \"{{ASSET_ID}}\"\r\n  }\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{ALICE_MANAGEMENT}}/v2/contractdefinitions",
							"host": [
								"{{ALICE_MANAGEMENT}}"
							],
							"path": [
								"v2",
								"contractdefinitions"
							]
						}
					},
					"response": []
				}
			]
		},
